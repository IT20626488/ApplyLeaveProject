<?xml version="1.0" encoding="UTF-8"?>


<web-app version="3.1" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee">




<servlet>

<servlet-name>login</servlet-name>

<servlet-class>apply.LoginServlet</servlet-class>

</servlet>


<servlet-mapping>

<servlet-name>login</servlet-name>

<url-pattern>/login</url-pattern>

</servlet-mapping>

<servlet>
<servlet-name>insert</servlet-name>
<servlet-class>apply.Userinsert</servlet-class>
</servlet>

<servlet-mapping>

<servlet-name>insert</servlet-name>

<url-pattern>/insert</url-pattern>

</servlet-mapping>
<servlet>

<servlet-name>update</servlet-name>

<servlet-class>apply.UpdateMemberServlet</servlet-class>

</servlet>


<servlet-mapping>

<servlet-name>update</servlet-name>

<url-pattern>/update</url-pattern>

</servlet-mapping>


<servlet>

<servlet-name>delete</servlet-name>

<servlet-class>apply.DeleteMemberServlet</servlet-class>

</servlet>


<servlet-mapping>

<servlet-name>delete</servlet-name>

<url-pattern>/delete</url-pattern>

</servlet-mapping>




<servlet-mapping>

<servlet-name>default</servlet-name>

<url-pattern>*.css</url-pattern>

</servlet-mapping>



</web-app>